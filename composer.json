{
  "name": "wyrihaximus/react-psr-7-stream",
  "description": "\ud83c\udf81 Decorate ReactPHP streams as PSR-7 streams",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "psr/http-message": "^1.0",
    "react/async": "^4.0",
    "react/promise-stream": "^1.5",
    "react/stream": "^1.2"
  },
  "require-dev": {
    "react/event-loop": "^1.3",
    "react/http": "^1.8",
    "wyrihaximus/async-test-utilities": "^5.0.23"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\React\\PSR7\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\React\\PSR7\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "icanhazstring/composer-unused": true,
      "infection/extension-installer": true,
      "composer-unused/composer-unused-plugin": true
    },
    "platform": {
      "php": "8.1.13"
    },
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
